record(ai,"$(P)$(DAU):$(LABEL):CommInput")
{
    field(PINI, "YES")
    field(DTYP, "asynFloat64")
    field(PREC, "3")
    field(INP,  "@asyn($(PORT),$(ADDR)) COMM_INPUT")
    field(SCAN, "I/O Intr")
}
record(ao,"$(P)$(DAU):$(LABEL):CommInput_Set")
{
    field(DTYP, "asynFloat64")
    field(PREC, "3")
    field(OUT,  "@asyn($(PORT),$(ADDR)) COMM_INPUT")
}

record(stringout,"$(P)$(DAU):$(LABEL):Label")
{
    field(PINI, "YES")
    field( OUT, "$(P)$(DAU):$(LABEL):CommInput.DESC PP")
}
