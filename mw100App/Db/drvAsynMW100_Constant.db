record(ai,"$(P)$(DAU):$(LABEL):Constant")
{
    field(PINI, "YES")
    field(DTYP, "asynFloat64")
    field(PREC, "3")
    field(INP,  "@asyn($(PORT),$(ADDR)) CONSTANT")
    field(SCAN, "I/O Intr")
}
record(ao,"$(P)$(DAU):$(LABEL):Constant_Set")
{
    field(DTYP, "asynFloat64")
    field(PREC, "3")
    field(OUT,  "@asyn($(PORT),$(ADDR)) CONSTANT")
}

record(stringout,"$(P)$(DAU):$(LABEL):Label")
{
    field(PINI, "YES")
    field( OUT, "$(P)$(DAU):$(LABEL):Constant.DESC PP")
}
