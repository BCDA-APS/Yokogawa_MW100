record(ai,"$(P)$(DAU):$(LABEL):ADC")
{
    field(DTYP, "asynFloat64")
    field(PREC, "4")
    field(INP,  "@asyn($(PORT),$(ADDR)) ADC")
    field(SCAN, "I/O Intr")
}

record(stringin,"$(P)$(DAU):$(LABEL):ADC_Unit")
{
    field(PINI, "YES")
    field(DTYP, "asynOctetRead")
    field(INP,  "@asyn($(PORT),$(ADDR)) ADC_UNIT")
    field(SCAN, "I/O Intr")
}

record(mbbi,"$(P)$(DAU):$(LABEL):ADC_Status")
{
    field(PINI, "YES")
    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT),$(ADDR)) ADC_STATUS")
    field(SCAN, "I/O Intr")
    field(ZRVL, "0")
    field(ZRST, "Skip")
    field(ONVL, "1")
    field(ONST, "Normal")
    field(TWVL, "2")
    field(TWST, "Diff")
}

record(mbbi,"$(P)$(DAU):$(LABEL):ADC_Alarm")
{
    field(PINI, "YES")
    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT),$(ADDR)) ADC_ALARM")
    field(SCAN, "I/O Intr")
    field(ZRVL, "0")
    field(ZRST, "None")
    field(ONVL, "1")
    field(ONST, "Overrange")
    field(TWVL, "2")
    field(TWST, "Underrange")
    field(THVL, "3")
    field(THST, "Skip/Off")
    field(FRVL, "4")
    field(FRST, "Error")
    field(FVVL, "5")
    field(FVST, "Uncertain")
}


record(stringout,"$(P)$(DAU):$(LABEL):Label")
{
    field(PINI, "YES")
    field( OUT, "$(P)$(DAU):$(LABEL):ADC.DESC PP")
}
